<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TestMapper">

	<insert id="insert" useGeneratedKeys="true" keyProperty="ID">
		INSERT
		INTO
		testTable(
		NAME,
		AGE
		)
		VALUES
		(
		#{name},
		#{age}
		)
	</insert>
</mapper>